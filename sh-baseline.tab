 /SCRIPTPATH
  . ./tools/sh/env.sh                          echo 1.2. $SCRIPTPATH

 . ./tools/ci/env.sh                           echo 2.1. $SCRIPTPATH
 /SCRIPTPATH
  . ./tools/ci/env.sh                          echo 2.2. $SCRIPTPATH

 export sh_tools=tools/sh ci_tools=tools/ci
