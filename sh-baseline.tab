sh_tools=tools/sh ci_tools=tools/ci            . ./tools/sh/util.sh
export sh_tools=tools/sh ci_tools=tools/ci
  . ./tools/sh/util.sh                         . ./tools/ci/util.sh

set -euo pipefail

